<nox-admin-localization>
  <div></div>

  <style scoped>
    :scope,
    input,
    button,
    *::after,
    *::before {
      font: 20px Helvetica, Arial, sans-serif;
    }

    :scope * {
      box-sizing: border-box;
    }
  </style>

  <script>
    const _self = this;

    function handleMount(ev){}

    this.on('mount', handleMount);
  </script>
</nox-admin-localization>